[global_config]
[keybindings]
  close_term = <Primary><Shift><Alt>w
  close_window = <Primary><Shift><Alt>q
[profiles]
  [[default]]
    use_system_font = False
    custom_command = TERM=xterm-256color bash -l
    background_image = None
    use_custom_command = True
    font = Source Code Pro for Powerline 10
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
